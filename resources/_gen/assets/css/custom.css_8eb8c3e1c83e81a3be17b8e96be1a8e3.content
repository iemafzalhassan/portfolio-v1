;opacity:0 !important;border-radius:40% 60% 60% 40%/70% 30% 70% 30% !important;box-shadow:0 1px 2px rgba(0,0,0,.1) !important}.marker-effect.elastic::after{transform:scaleX(0) !important;transform-origin:left !important;transition:transform .6s cubic-bezier(.68,-.55,.265,1.55),opacity .3s !important}.marker-effect.elastic:hover::after{transform:scaleX(1) !important;opacity:.7 !important}.marker-effect:hover{color:#fff !important}.dark-toggle{position:relative !important;display:flex !important;align-items:center !important;padding-left:1.5rem !important;margin-left:1rem !important;font-weight:500 !important;cursor:pointer !important;color:#fff !important}.dark-toggle-icon{position:absolute !important;left:0 !important;display:flex !important;align-items:center !important;justify-content:center !important;width:1.5rem !important;height:1.5rem !important;overflow:hidden !important;border-bottom:1px solid transparent !important;transition:all .3s ease !important}.dark-toggle:hover .dark-toggle-icon{border-color:#9ca3af !important}.dark-toggle svg{width:1.5rem !important;height:1.5rem !important;transition:transform .7s ease !important;color:#fff !important}.dark-toggle:hover svg{transform:translateY(10px) !important}.mobile-menu-toggle{display:none !important;flex-direction:column !important;align-items:center !important;justify-content:center !important;width:1.5rem !important;height:1.5rem !important;margin-left:1rem !important;cursor:pointer !important}.mobile-menu-toggle svg{width:2rem !important;height:2rem !important;color:#fff !important}@media(max-width:640px){.mobile-menu-toggle{display:flex !important}.custom-nav{display:none !important}.custom-nav.mobile-open{position:fixed !important;top:5rem !important;left:0 !important;display:flex !important;flex-direction:column !important;width:100% !important;background:rgba(0,0,0,.95) !important;backdrop-filter:blur(10px) !important;border-bottom:1px solid rgba(255,255,255,.1) !important;padding:1rem !important}.nav-link{width:100% !important;padding:.75rem !important;text-align:center !important}}html{background-color:#0f0f23 !important;color:#fff !important}body{background-color:#0f0f23 !important;color:#fff !important;margin-top:5rem !important}:root{--color-background:#0f0f23 !important;--color-text:white !important;--color-text-muted:#9ca3af !important;--color-primary:#a855f7 !important;--color-border:rgba(255, 255, 255, 0.1) !important}.custom-button{display:inline-flex !important;width:auto !important;padding:.5rem 1rem !important;margin-top:1.25rem !important;font-size:.75rem !important;font-weight:600 !important;color:#fff !important;background:0 0 !important;border:1px solid #fff !important;border-radius:9999px !important;text-decoration:none !important;transition:all .3s ease !important}.custom-button:hover{color:#0f0f23 !important;background:#fff !important;border-color:#fff !important}@keyframes heartbeat{0%{transform:scale(1)}50%{transform:scale(1.2)}100%{transform:scale(1)}}.animate-pulse{animation:heartbeat 1s infinite !important}html.dark{color-scheme:dark !important}.blowfish-header,.blowfish-nav,.blowfish-menu{display:none !important}header:not(.custom-header),nav:not(.custom-nav){display:none !important}